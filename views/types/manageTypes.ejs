<html>
    <head>
        <title>Manage types</title>
    </head>
    <body>
        <a href="/create/type">Add new type</a>
        <ul>
        <% types.forEach((type)=>{ %>
            <li>
                id: <%=type.id%>
                name: <%=type.type_name%>
                <a href="/update/type/<%=type.id%>">update</a>
                <form action="/delete/type/<%=type.id%>" method="post">
                    <button type="submit" onclick="return confirm('Are you sure, you want to delete this type?')">Delete</button>
                </form>
            </li>
        <% }) %>
        </ul>  
        <a href="/">Back to Home</a>  
    </body>
</html>